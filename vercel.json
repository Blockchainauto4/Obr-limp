
{
  "redirects": [
    {
      "source": "/tag/(.*)",
      "destination": "/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    },
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/servico/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/servicos/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/atendimento/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/((?!api/|robots\\.txt|sitemap\\.xml|assets/|favicon\\.ico|.*\\..*$).*)",
      "destination": "/index.html"
    }
  ]
}
